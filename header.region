  </head>
  <body id="{squarespace.page-id}" class="{squarespace.page-classes}">

    <div class="mobile-nav">
      <squarespace:navigation navigationId="headerNav" template="navigation-mobile" />
    </div>

    <main>

      <nav>
        <div class="nav-wrapper padding-95">
        <!--Site title or logo.-->
        {.section website}
          <div class="logo">
            <a href="/">
              {.section logoImageUrl}
                 <img class="logo" src="{logoImageUrl}?format=original" alt="{siteTitle}" />
              {.or}
                {siteTitle}
              {.end}
            </a>
          </div>
        {.end}

        <!--Main navigation.-->
        <squarespace:navigation navigationId="headerNav" template="navigation" />
        </div>
        <div class="menu-burger">
          <div class="cheeseburger">
            <span class="a color"></span>
            <span class="b color"></span>
            <span class="c color"></span>
          </div>
        </div>
      </nav>
